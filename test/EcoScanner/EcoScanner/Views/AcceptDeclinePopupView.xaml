<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="EcoScanner.Views.AcceptDeclinePopupView">
  <ContentView.Content>
        <StackLayout>
            <Grid RowDefinitions="*" ColumnDefinitions="*, *">
                <Image Source="StorTilbageKnap.png" Grid.Row="0" Grid.Column="0"/>
                <ImageButton Command="{Binding BackPressed}" Source="StorTilbageKnap.png" Opacity="0" Grid.Row="0" Grid.Column="0"/>
                <Image Source="TilHjemmeside.png" Grid.Row="0" Grid.Column="1"/>
                <ImageButton Command="{Binding ClearListPressed}" Source="TilHjemmeside.png" Opacity="0" Grid.Row="0" Grid.Column="1"/>
            </Grid>
        </StackLayout>
    </ContentView.Content>
</ContentView>